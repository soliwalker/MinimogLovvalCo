/* THEME SHINEON */
@import "{{ 'so-theme.css' | asset_url }}";
@import "{{ 'tailwind-shineon.css' | asset_url }}";

div:empty, section:empty {
  display: block;
}

$font_modal_text: "PT Sans", "Avenir Next Rounded", Helvetica, Arial, sans-serif;
// TITLE font family
{% if settings.default_font_family != "" and settings.default_font_family != null %}
$font_header: "{{ settings.default_font_family }}", Helvetica, Arial, sans-serif;
{% else %}

$font_header: Raleway, "Avenir Next Rounded", Helvetica, Arial, sans-serif;
{% endif %}

// Colors

{% if settings.add_to_cart_bg_color != "" and settings.add_to_cart_bg_color != null %}
$color_atc: {{ settings.add_to_cart_bg_color }};
{% else %}
$color_atc: #00b70f;
{% endif %}
{% if settings.add_to_cart_hover_bg == "lighten" %}
$color_atc_hover: lighten($color_atc, 5%);
{% else %}
$color_atc_hover: darken($color_atc, 17%);
{% endif %}

$product_price_font_style: 400;
$product_price_color: #000000;
$product_price_font_size: 28px;
$product_tabs_font_size: 15px;
$product_price_save_size: 15px;
$product_title_font_style: 400;
$add_to_cart_font_style: 700;
$add_to_cart_font_color: #fff;
$add_to_cart_font_size: 16px;
$flash_sold_text_font_size:16px;
$flash_sold_font_style:600;
$flash_sold_text_color:#f03c07;
$add_to_cart_font_color: #fff;

$border_radius: 0;

// ------------------------
// General CSS ~ typography

@import url(https://fonts.googleapis.com/css?family=Tangerine:700);
@import url(https://fonts.googleapis.com/css2?family=Quattrocento:wght@700&display=swap);
@font-face {
  font-family: 'Spartan MB Extra Bold';
  src: url('theme-shineon-SpartanMB-Extra-Bold.otf');
}

#ProductSection-product-template-shineon-e-and-g-upsell,
#ProductSection-product-template-shineon {
