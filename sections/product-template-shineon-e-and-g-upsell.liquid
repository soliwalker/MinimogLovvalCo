{% for variant in product.variants %}
  {% if variant.metafields.shineon.ipcs contains 'E035T' %}
    {% assign sided_necklace = true %}
  {% endif %}
  {% comment %} 3307 CRO'd new PP design {% endcomment %}
  {% if section.settings.design == 'cro-tested' %}
    {% for ipcs_3307 in cro_design_product_for_ipcs_arr %}
      {% if variant.metafields.shineon.ipcs contains ipcs_3307 %}
        {% assign cro_design_product = ipcs_3307 %}
        {% break %}
      {% endif %}
    {% endfor %}
  {% endif %}
  {% if variant.metafields.shineon.engravings > 0 %}
    {% assign has_engraving = true %}
  {% endif %}
  {% if variant.metafields.shineon.ipcs contains 'E060S' or variant.metafields.shineon.ipcs contains 'E060SG' or variant.metafields.shineon.attachment contains 'sweetest-{{ variant_accessory_type }}s-necklace' or variant.metafields.shineon.ipcs contains 'C30102' %}
    {% assign sweetest_product_design = true %}
  {% endif %}
  {% if one_style == 1 and product.variants.size >= 8 and variant.metafields.shineon.type != so_current_variant.metafields.shineon.type %}
    {% assign variant_id_arr = variant.id | split: '--array-of-one--' %}
    {% assign variant_image_id_arr = variant.image.id | split: '--array-of-one--' %}
    {% assign hidden_variants = hidden_variants | concat: variant_id_arr %}
    {% assign hidden_images = hidden_images | concat: variant_image_id_arr %}
  {% endif %}

  {% if variant.metafields.shineon.ipcs contains '-WOOD' %}
    {% assign has_box = true %}
    {% assign box_option_index = 1 %}
    {% assign box_option = 'option2' %}
    {% if product.options_with_values.size > 1 %}
      {% assign is_options_detected = true %}
    {% endif %}
  {% endif %}

  {% if variant.title contains '18' and sweetest_product_design != true %}
    {% if variant.metafields.shineon.gold == '1' %}
      {% assign add_upsell_modal = variant %}
    {% endif %}
  {% endif %}
  {% if variant.metafields.shineon.engravable == 0 or variant.metafields.shineon.gold == 1 and sweetest_product_design != true %}
    {% if variant.metafields.shineon.gold == 1 or variant.title contains '18' and product.variants.size > 1 %}
      {% assign add_upsell_modal = variant %}
    {% endif %}
  {% endif %}
  {% if variant.metafields.shineon.metal == 'other' %}
    {% unless variant.title contains '18' %}
      {% assign regular_variant_id = variant.id %}
    {% endunless %}
  {% else %}
    {% if variant.metafields.shineon.gold != 1 %}
      {% assign regular_variant_id = variant.id %}
    {% endif %}
  {% endif %}

  {% if variant.metafields.shineon.attachment contains 'cross' and variant.metafields.shineon.shape == 'cross' %}
    {% assign is_cross = variant %}
  {% endif %}

  {% if has_engraving %}
    {% if variant.metafields.shineon.engravable != nil %}
      {% if variant.metafields.shineon.engravable == 1 %}
        {% assign is_options_detected = true %}
        {% assign engraving_option_yes = variant[engraving_option] %}
      {% elsif variant.metafields.shineon.engravable == 0 %}
        {% assign engraving_option_no = variant[engraving_option] %}
      {% endif %}
    {% endif %}
  {% endif %}

  {% if base_variant == false or base_variant.price > variant.price and is_template_optimized == true %}
    {% assign base_variant = variant %}
  {% endif %}

  {% if variant.metafields.shineon.gold == 1 %}
    {% assign has_gold_variants = true %}
    {% assign gold_variant_price = variant.price %}
  {% endif %}

  {% unless variant.metafields.shineon.gold == 1 %}
    {% assign has_silver_variants = true %}
    {% assign not_gold_variant_price = variant.price %}
  {% endunless %}

  {% if variant.metafields.shineon.metal != 'other' %}
    {% if variant.metafields.shineon.gold != nil %}
      {% assign metal = variant.metafields.shineon.gold | split: '--array-of-one--' %}
    {% else %}
      {% assign metal = "" | split: '--empty-array--' %}
    {% endif %}
  {% endif %}
  {% assign metals = metals | concat: metal %}
  {% assign variant_option1_arr = variant.option1 | split: '--array-of-one--' %}
  {% assign option1_list = option1_list | concat: variant_option1_arr %}
  {% assign variant_option2_arr = variant.option2 | split: '--array-of-one--' %}
  {% assign option2_list = option2_list | concat: variant_option2_arr %}
  {% assign variant_option3_arr = variant.option3 | split: '--array-of-one--' %}
  {% assign option3_list = option3_list | concat: variant_option3_arr %}

  {% if has_engraving and variant.metafields.shineon.engravable %}
  {% assign engraved_variants_arr = engraved_variants_arr | append: '--next-prop--' | append: variant.metafields.shineon.metal | append: '---' | append: variant.featured_image | append: '---' | append: variant.price %}
  {% endif %}

{% endfor %}

{% if product.options_with_values.size > 1 and has_engraving == true %}
