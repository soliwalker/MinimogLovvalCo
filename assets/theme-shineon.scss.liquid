body.loading:after {
  content: "";
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #fff;
  z-index: 10;
}
img {
  vertical-align: top;
}

// ---------------------------------------
// SIMPLE GRID - SASS/SCSS ~ simplegrid.io

$so-width: 96%;
$so-gutter: 4%;
$so-breakpoint-xsmall: 22.5em; // 360px
$so-breakpoint-small: 33.75em; // 540px
$so-breakpoint-med: 45em; // 720px
$so-breakpoint-xmd: 60em; // 960px
$so-breakpoint-large: 75em; // 1200px ~ 60em is 960px

.so-container {
  width: 100%;
  margin-left: auto !important;
  margin-right: auto !important;

  @media only screen and (min-width: $so-breakpoint-small) {
    width: 100%;
  }

  @media only screen and (min-width: $so-breakpoint-large) {
    width: 100%;
    max-width: 1170px; // 1200px
  }
  &.so-product-wrap {
    margin-top: 0;
    padding-top: 0;
  }
}
.so-row {
  position: relative;
  width: 100%;
}

.so-row.so-row-fluid {
  [class^="so-col"] {
    margin-left: 0;
    margin-right: 4%;
  }
  .so-col-12{
    margin: 2% 0 0.3rem;
  }
}

.so-row [class^="so-col"],
.so-row [class*="so-col"] {
  float: left;
  margin: 0.5rem 2%;
  min-height: 0.125rem;
}

.so-row:not(.so-row-flex)::after {
  content: "";
  display: table;
  clear: both;
}
.so-row-flex {
  display: flex;
}

.so-visible-sm {
  display: none !important;
}

.so-visible-md {
  display: none !important;
}

.so-visible-xmd {
  display: none !important;
}

.so-col-1,
.so-col-2,
.so-col-3,
.so-col-4,
.so-col-5,
.so-col-6,
.so-col-7,
.so-col-8,
.so-col-9,
.so-col-10,
.so-col-11,
.so-col-12 {
  width: $so-width;
}

.so-col-1-sm { width:($so-width / 12) - ($so-gutter * 11 / 12); }
.so-col-2-sm { width: ($so-width / 6) - ($so-gutter * 10 / 12); }
.so-col-3-sm { width: ($so-width / 4) - ($so-gutter * 9 / 12); }
.so-col-4-sm { width: ($so-width / 3) - ($so-gutter * 8 / 12); }
.so-col-5-sm { width: ($so-width / (12 / 5)) - ($so-gutter * 7 / 12); }
.so-col-6-sm { width: ($so-width / 2) - ($so-gutter * 6 / 12); }
.so-col-7-sm { width: ($so-width / (12 / 7)) - ($so-gutter * 5 / 12); }
.so-col-8-sm { width: ($so-width / (12 / 8)) - ($so-gutter * 4 / 12); }
.so-col-9-sm { width: ($so-width / (12 / 9)) - ($so-gutter * 3 / 12); }
.so-col-10-sm { width: ($so-width / (12 / 10)) - ($so-gutter * 2 / 12); }
.so-col-11-sm { width: ($so-width / (12 / 11)) - ($so-gutter * 1 / 12); }
.so-col-12-sm { width: $so-width; }

@media only screen and (min-width: $so-breakpoint-med) {
  .so-col-1 { width:($so-width / 12) - ($so-gutter * 11 / 12); }
  .so-col-2 { width: ($so-width / 6) - ($so-gutter * 10 / 12); }
  .so-col-3 { width: ($so-width / 4) - ($so-gutter * 9 / 12); }
  .so-col-4 { width: ($so-width / 3) - ($so-gutter * 8 / 12); }
  .so-col-5 { width: ($so-width / (12 / 5)) - ($so-gutter * 7 / 12); }
  .so-col-6 { width: ($so-width / 2) - ($so-gutter * 6 / 12); }
  .so-col-7 { width: ($so-width / (12 / 7)) - ($so-gutter * 5 / 12); }
  .so-col-8 { width: ($so-width / (12 / 8)) - ($so-gutter * 4 / 12); }
  .so-col-9 { width: ($so-width / (12 / 9)) - ($so-gutter * 3 / 12); }
  .so-col-10 { width: ($so-width / (12 / 10)) - ($so-gutter * 2 / 12); }
  .so-col-11 { width: ($so-width / (12 / 11)) - ($so-gutter * 1 / 12); }
  .so-col-12 { width: $so-width; }

  .so-visible-md {
    display: block !important;
  }
  .so-hidden-md {
    display: none !important;
  }
}
.so-icons{
  display: inline-block;
  font-style: normal;
  font-variant: normal;
  text-rendering: auto;
  line-height: 1;
  font-size: 1em;
  width: 1.125em;
  color: inherit;
  vertical-align: middle;
}
span.money {
  font-weight: normal;
}
span.so_nobreak {
  white-space: nowrap;
}
.so-product-pricing {
  line-height: 1.6em;
  margin-bottom: 0.5em;
}
#so-compare-at-label-wrap, .so-compare-at-label-wrap {
  color: #c40000;
  text-shadow: none;
  font-weight: normal;
  @media(min-width: 992px) {
    margin-left: 10px;
  }
  @media(max-width: 767px) {
    margin-right: 10px;
  }
  .so-compare-at-label {
    text-decoration: line-through;
  }
}
#so-compare-at-savings-label, .so-compare-at-savings-label {
  color: #fff;
  background-color: #00b70f;
  padding: 0 5px;
  margin-left: 10px;
}
.so-container {}
.so-product-left, .so-product-right {
  vertical-align: top;
}
@media only screen and (max-width: 45em) {
  .so-row [class^="so-col"] {
    &.so-product-left {
      margin-bottom: 0;
    }

    &.so-product-right {
      margin-top: 0;
    }
  }
}
.main-content {
  margin-top: 0;
  padding: 0;

  body:not(.template-index) & {
    padding-top: 0 !important;/* Brooklyn theme fix */
  }

  @media(min-width: 1200px) {
    &.wrapper,
    .wrapper {
      min-width: 1170px;
      max-width: 100%;
    }
  }
}
.so-product {
  .so-banner-top {
    text-align:center;
  }
  &.so-row {
    .so-col-10:first-child:last-child {
      margin: 0.5rem auto !important;
      float: none !important;
    }
  }
  @media screen and (max-width: 500px)
  {
    .product__price.with-quantity-selector
    {
      padding-left: 0!important;
      justify-content: flex-start!important;
    }
    .so-product-price.with-cro-design
    {
      flex-direction: row!important;
      position: relative;
    }
    .so-product-title.with-cro-design
    {
      max-width: 75%;
    }
  }
  .so-btn-add-to-cart-with-quantity
  {
    display: flex;
    align-items: center;
    
    .product-quantity-w,
    .so-quantity-selector-wrap,
    .so-quantity-selector,
    .so-cart-qty-input
    {
      min-height: 50px;
    }
    .so-quantity-selector-wrap
    {
      margin-top: 0!important;
    }
    .so-cart-qty-input
    {
      height: 100%;
    }
    .so-btn-add-to-cart
    {
      margin-top: 0;
      margin-left: 10px;
      min-width: initial;
    }
  }
  .so-btn-add-to-cart-wrap {
    max-width: 100%;
  }
  a.so-ring-size-help-link {
     color: blue;
  }
  .so-variant-change-ring-size {
    select {
      border-radius: $border_radius;
      width: 100%;
    }
  }
  #ringsizer {
    min-height: 40px !important;
    line-height: 40px !important;
    text-transform: uppercase !important;
    border-radius: $border_radius !important;
    border: 2px solid #deba75 !important;
    display: block !important;
    padding: 0 15px 0 15px !important;
    span.find-my-ring-size-icon-wrap {
      svg {
        width: 27px;
      }
    }
  }
  .so-field-wrap {
    margin-top: 15px;
    &.so-engraving-line1-wrap, &:first-child {
      margin-top: 0;
    }
    // Label above the swatch / dropdown selectors
    .so-option-label {
      font-size: 14px;
      .so-option-number {
        width: 1.5em;
        height: 1.5em;
        line-height: 1.5em;
        background-color: #d94900;
        border-radius: 50%;
        display: inline-block;
        text-align: center;
        color: #ffffff;
        margin-right: 0.5em;
      }
      &.so-option-label-styled {
        font-family: $font_header;
        font-weight: 700;
        margin: 0 0 5px 0;
      }
    }
// Swatches Metal color
    .so-swatch {
      .so-swatch-metal-icon {
        display: none;
      }
    }
    .so-swatch.hide-metal-swatch {
      .so-swatch-metal {
        display: none;
      }
    }
    .so-swatch:not(.hide-metal-swatch):not(.so-swatch-with-box):not(.cro-design-product-true)
    {
      .so-label-symbol-wrap {
        display: flex;
        justify-content: center;
        align-items: center;

        .so-swatch-metal
        {
          border: 2px solid #fff;
          border-radius: 100%;
          box-shadow: 0px 4px 4px #d2dbe8;
          display: inline-block;
          width: 20px;
          flex: 0 0 20px;
          height: 20px;
          margin-right: 8px;
          margin-top: -2px;

          &.so-swatch-metal-silver,
          &.so-swatch-metal-other,
          &.so-swatch-metal-stainless
          {
            background: linear-gradient(45deg, rgba(160,160,160,1) 0%,rgba(232,232,232,1) 56%);
          }
          //Screwdriver
          &.so-swatch-metal-black
          {
            background: linear-gradient(45deg, rgba(160,160,160,1) 0%,rgba(50,50,50,1) 56%);
          }
          &.so-swatch-metal-gold,
          &.so-swatch-metal-gold14solid,
          &.so-swatch-metal-gold14
          {
            background: radial-gradient(ellipse farthest-corner at right bottom, #FEDB37 0%, #FDB931 20%, transparent 80%), radial-gradient(ellipse farthest-corner at left top, #FFFFFF 0%, #FFFFAC 8%, #D1B464 100%);
          }
          .so-swatch-metal-icon {
            display: none;
          }
        }
      }
    }
    .so-swatch.metal-swatch-svg:not(.so-swatch-with-box) {
      .so-swatch-metal {
        display: none!important;
      }
      .so-swatch-metal-icon {
        border: 2px solid #fff;
        border-radius: 100%;
        box-shadow: 0px 4px 4px #d2dbe8;
        display: flex;
        align-items: center;
        justify-content: center;
        width: 20px;
        flex: 0 0 20px;
        height: 20px;
        margin-right: 8px;
        margin-top: -2px;
      }
    }
    // Swatches / dropdown selectors
    .so-option-fieldset {
      * {
        box-sizing: border-box;
      }
      text-align: center;
      padding: 0;
      margin-bottom: 0.5rem;
      min-height: 0;
      border: none;
      &.so-swatch-metal-picker {
        padding: 0;
        border: none;
      }
      .so-variant-change-metal,
      .so-variant-change {
        display: flex;

        &.out-of-stock {
          label.so-swatch {
            color: #cdcdcd;

            &:hover {
              cursor: default;
              .notification {
                display: block;
              }
            }

            .so-label-symbol-wrap {
              position: relative;
              white-space: nowrap;

              &:hover {
                .notification {
                  display: inline-block;
                }
              }
              .notification {
                display: inline-block;
                color: #555;
              }

              @media (min-width: 1200px) {
                .notification {
                  display: none;
                  position: absolute;
                  top: -120%;
                  left: 50%;
                  transform: translateX(-50%);
                  background-color: #e2e2e2;
                  padding: 2px 10px;
                  color: #555;
                }
              }
            }
          }
        }
      }
      // The direct children of the fieldset we'll hide/show
      .so-variant-change, .so-variant-change-metal {
        &.so-hidden, &.so-inactive-metal {
          display: none !important;
        }
      }
      .text-overflowed {
        & + .text-overflowed {
          margin-top: 5px;
        }
        label.so-swatch {
          @media(min-width: 768px) and (max-width: 991px) {
            white-space: normal !important;
          }
          span {
            white-space: normal;
          }
          .so-translation-with-metal {
            justify-content: center;

            .metal {
              margin-top: -3px;
              margin-right: 10px;
            }
          }
        }
      }
      .so-col-6.so-variant-change-metal:only-child {
        width: 100%!important;
      }
      // The bordered label swatch
      label.so-swatch {
        cursor: pointer;
        margin: 0;
        background-color: #fff;
        border: 1px solid #999999;
        padding: 6px 15px;
        text-align: center;
        width: 100%;
        color: #000;
        font-size: 14px;
        display: inline-block;
        box-shadow: 0px 4px 4px rgba(0, 0, 0, 0.25);
        &.so-swatch-metal {

        }
        // The inner text or symbols within the label swatch
        span {
          line-height:20px;
          margin-bottom: 0 !important;
          display: inline-block;
          font-weight: 500;
          text-transform: capitalize;
          white-space: nowrap;
        }
        // Swatches with symbols have a special wrap.
        div.so-label-symbol-wrap {
          line-height: 20px;
          width: 100%;
        }
        &.so-swatch-with-box {
          white-space: nowrap;
        }
      }
    }
    select.so-variant-selector {
      width:96%;
      border-radius: $border_radius;
      padding: 12px 16px 8px 16px;
      min-height: 40px;
      line-height: 1 !important;
      font-weight: bold;
      text-transform: uppercase;
      color: #000000;
      font-size: 14px;
      option {
        &.so-hidden, &.so-inactive-metal {
          display: none !important;
        }
      }
    }
    &.so-custom-property-flex-wrap {
      * {
        box-sizing: border-box;
      }
      margin-top: 0;
      display: -webkit-box;
      display: -webkit-flex;
      display: -ms-flexbox;
      display: flex;
      -webkit-box-orient: horizontal;
      -webkit-box-direction: normal;
      -webkit-flex-direction: row;
      -ms-flex-direction: row;
      flex-direction: row;
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
      -ms-flex-pack: justify;
      justify-content: space-between;
      -webkit-box-align: center;
      -webkit-align-items: center;
      -ms-flex-align: center;
      align-items: center;
      &.so-engraving-lines {
        -webkit-box-align: end;
        -webkit-align-items: flex-end;
        -ms-flex-align: end;
        align-items: flex-end;
      }
      > div {
        -webkit-box-flex: 0;
        -webkit-flex: 1 1 49%;
        -ms-flex: 1 1 49%;
        flex: 1 1 49%;
        .so-engraving-input {
          padding: 5px;
          font-size: 16px;
          width: 100%;
          font-family: 'Tangerine', serif !important;
          font-weight: bold;
          border: 1px solid #e2e2e2;
          -webkit-border-radius: $border_radius;
          -moz-border-radius: $border_radius;
          border-radius: $border_radius;
          text-align: center;
        }
        &.so-variant-change-ring-size{
          -webkit-box-flex: 0.9;
          -webkit-flex-grow: 0.9;
          -ms-flex-positive: 0.9;
          flex-grow: 0.9;
        }
      }
      &[data-shape='cross']{
        div.so-engraving-input-wrap {
          -webkit-box-flex: 0.9;
          -webkit-flex-grow: 0.9;
          -ms-flex-positive: 0.9;
          flex-grow: 0.9;
          &.so-engraving-line1-wrap {
            max-width: 40%;
            .so-engraving-input {
              font-family: 'Spartan MB Extra Bold', sans-serif;
            }
          }
        }
      }
      .so-engraving-input-wrap {
        label {
          font-weight: 500;
          margin-bottom: 0;
        }
      }
    }
  }
  .so-option-label-metal + .so-field-wrap {
    margin-top: 0;
  }
  @media only screen and (max-width: 1024px) {
    #so-step-buttons .so-bu-buttons {
      font-size: 0.9em !important;
    }
  }
  ul.so-tabs, ul.so-tabs-content {
    margin: 0 0 15px 0;
    > li {
      list-style-type: none;
      p {
        margin-bottom: 10px;
      }
    }
    padding: 0;
  }
  ul.so-tabs-product {
    font-size: 0;
    li {
      border-top: 1px solid #eee;
      display: inline-block;
      width: 50%;
      margin-right: 0;
      line-height: 30px;
      font-size: 15px;
      text-transform: uppercase;
      text-align: center;
      &.so-active {
        border-bottom: 2px solid #666;
      }
    }
  }
  .so-tabs .so-active {
    font-weight: 700;
  }
}
ul.so-tabs-content.so-tabs-content-product {
  li {
    img {
      max-width: 100%;
    }
